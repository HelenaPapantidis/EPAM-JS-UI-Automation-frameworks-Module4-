{
  "name": "epam-js-ui-automation-frameworks-module4",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.19.2",
    "@wdio/cli": "^9.19.2",
    "@wdio/cucumber-framework": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.js",
    "test:login": "npx wdio run ./wdio.conf.js --spec ./src/features/login.feature --capabilities chrome",
    "test:basket": "npx wdio run ./wdio.conf.js --spec ./src/features/basket.feature --capabilities chrome",
    "test:chrome": "wdio run ./wdio.conf.js --spec ./src/features",
    "test:firefox": "wdio run ./wdio.conf.js --spec ./src/features --capabilities firefox",
    "test:edge": "wdio run ./wdio.conf.js --spec ./src/features --capabilities MicrosoftEdge"
  }
}
